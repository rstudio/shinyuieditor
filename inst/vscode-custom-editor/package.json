{
  "name": "shinyuieditor-vscode-extension",
  "description": "VSCode extension for running shinyuieditor",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rstudio/shinyuieditor/"
  },
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCustomEditor:shinyUiEditor.appFile"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "shinyUiEditor.appFile",
        "displayName": "ShinyUiEditor",
        "selector": [
          {
            "filenamePattern": "*app.R"
          }
        ]
      }
    ],
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "yarn compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -w -p ./"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.65.0",
    "@types/winreg": "^1.2.31",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "winreg": "^1.2.4",
    "shinyuieditor-client": "0.1.0"
  },
  "packageManager": "yarn@3.2.3"
}
